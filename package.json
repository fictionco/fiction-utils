{
  "name": "@fiction/utils",
  "type": "module",
  "version": "1.0.3",
  "description": "Well-tested utility functions",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/fictionco/fiction-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fictionco/fiction-utils.git"
  },
  "bugs": {
    "url": "https://github.com/fictionco/fiction-utils/issues"
  },
  "keywords": [
    "typescript",
    "utilities",
    "utils",
    "helpers",
    "functions",
    "ui",
    "reset",
    "modal",
    "dropdown",
    "click-outside",
    "escape-key",
    "framework-agnostic",
    "ssr"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:post-build": "vitest run test/post-build.test.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "ci": "git pull && npm test -- --run && npm run lint && npm run typecheck && git push",
    "prepublishOnly": "pnpm run build && pnpm run test && pnpm run test:post-build && pnpm run lint && pnpm run typecheck"
  },
  "dependencies": {
    "@types/spark-md5": "^3.0.5",
    "fast-safe-stringify": "^2.1.1",
    "spark-md5": "^3.0.2",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.0.0",
    "@types/node": "^22.5.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "bumpp": "^9.5.0",
    "dotenv": "^16.4.0",
    "eslint": "^9.9.0",
    "happy-dom": "^15.11.7",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^2.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
